>\n\n  <div class=\"article-info\" v-for=\"article in articles.reverse()\" :key=\"article.id\">\n    <h2>{{article.title}}</h2>\n    <p id=\"author-info\">Written by {{article.author}} on {{article.date}}</p>\n    <p id=\"featured-summary\">{{article.article}}</p>\n  </div>\n\n</div>\n</template>\n\n\n<script>\nimport axios from 'axios';\nexport default {\n  name: 'Home',\n  data() {\n    return {\n      articles: [],\n    }\n  },\n  created() {\n    this.getArticles();\n  },\n  methods: {\n    async getArticles() {\n      try {\n        let response = await axios.get(\"/api/articles\");\n        this.articles = response.data;\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    }\n  }\n}\n</script>\n\n<style media=\"screen\">\n.article-info {\n  /* background-color: #F0f0f0; */\n  margin: auto 20em;\n  margin-bottom: 3em;\n}\n\n#author-info {\n  margin-top: -1em;\n}\n\n#featured-summary {\n  margin-top: -.5em;\n  text-align: justify;\n}\n\n@media only screen and (max-width: 600px) {\n  .article-info {\n    margin-left: 1.5em;\n    margin-right: 1.5em;\n  }\n}\n</style>\n"],"sourceRoot":"src/views"}]}